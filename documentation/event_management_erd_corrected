// Event Management System ER Diagram
digraph EventManagementERD_Corrected {
	rankdir=LR splines=ortho
	node [fontname=Helvetica shape=box style=rounded]
	edge [fontname=Helvetica fontsize=10]
	USER [label="{USER |<u>UserID (PK)</u>\nUsername\nEmail\nPasswordHash\nRole\nProfileImageURL}" shape=record]
	EVENT [label="{EVENT |<u>EventID (PK)</u>\nTitle\nDescription\nStartDate\nEndDate\nTotalCapacity}" shape=record]
	VENUE [label="{VENUE |<u>VenueID (PK)</u>\nName\nAddress\nCapacity}" shape=record]
	CITY [label="{CITY |<u>CityID (PK)</u>\nName}" shape=record]
	CATEGORY [label="{CATEGORY |<u>CategoryID (PK)</u>\nName}" shape=record]
	TICKET [label="{TICKET |<u>TicketID (PK)</u>\nType\nPrice\nQuantityAvailable}" shape=record]
	BOOKING [label="{BOOKING |<u>BookingID (PK)</u>\nUserID (FK)\nEventID (FK)\nBookingDate\nNumberOfTickets\nTotalPrice}" fillcolor=lightyellow shape=record style="rounded,filled"]
	REVIEW [label="{REVIEW |<u>ReviewID (PK)</u>\nUserID (FK)\nEventID (FK)\nRating\nComment\nDatePosted}" fillcolor=lightyellow shape=record style="rounded,filled"]
	Organizes [label=Organizes fillcolor=lightblue shape=diamond style=filled]
	Makes [label=Makes fillcolor=lightblue shape=diamond style=filled]
	Writes [label=Writes fillcolor=lightblue shape=diamond style=filled]
	Has [label=Has fillcolor=lightblue shape=diamond style=filled]
	Receives [label=Receives fillcolor=lightblue shape=diamond style=filled]
	Gets [label=Gets fillcolor=lightblue shape=diamond style=filled]
	Hosts [label=Hosts fillcolor=lightblue shape=diamond style=filled]
	Located_In [label="Located In" fillcolor=lightblue shape=diamond style=filled]
	Belongs_To [label="Belongs To" fillcolor=lightblue shape=diamond style=filled]
	USER -> Organizes [arrowhead=none arrowtail=none dir=both]
	Organizes -> EVENT [arrowhead=crow arrowtail=tee dir=both]
	USER -> Makes [arrowhead=none arrowtail=none dir=both]
	Makes -> BOOKING [arrowhead=crow arrowtail=tee dir=both]
	USER -> Writes [arrowhead=none arrowtail=none dir=both]
	Writes -> REVIEW [arrowhead=crow arrowtail=tee dir=both]
	EVENT -> Has [arrowhead=none arrowtail=none dir=both]
	Has -> TICKET [arrowhead=crow arrowtail=tee dir=both]
	EVENT -> Receives [arrowhead=none arrowtail=none dir=both]
	Receives -> BOOKING [arrowhead=crow arrowtail=tee dir=both]
	EVENT -> Gets [arrowhead=none arrowtail=none dir=both]
	Gets -> REVIEW [arrowhead=crow arrowtail=tee dir=both]
	VENUE -> Hosts [arrowhead=none arrowtail=none dir=both]
	Hosts -> EVENT [arrowhead=crow arrowtail=tee dir=both]
	CITY -> Located_In [arrowhead=none arrowtail=none dir=both]
	Located_In -> VENUE [arrowhead=crow arrowtail=tee dir=both]
	CATEGORY -> Belongs_To [arrowhead=none arrowtail=none dir=both]
	Belongs_To -> EVENT [arrowhead=crow arrowtail=tee dir=both]
}
